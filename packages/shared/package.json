{
  "name": "@aisc/shared",
  "version": "0.1.0",
  "description": "Shared business logic for AI Shopping Copilot",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./interactor": {
      "types": "./dist/interactor/index.d.ts",
      "import": "./dist/interactor/index.js"
    },
    "./flows": {
      "types": "./dist/flows/index.d.ts",
      "import": "./dist/flows/index.js"
    },
    "./patterns": {
      "types": "./dist/patterns/index.d.ts",
      "import": "./dist/patterns/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "shopping",
    "automation",
    "shared"
  ],
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
